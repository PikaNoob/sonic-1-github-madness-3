000D0E3A                            ; ---------------------------------------------------------------------------
000D0E3A                            ; Wait for DMA
000D0E3A                            ; ---------------------------------------------------------------------------
000D0E3A                            
000D0E3A                            DMAWAIT macro ctrl
000D0E3A                            .Wait\@:
000D0E3A                            	if narg>0
000D0E3A                            		btst	#1,1(\ctrl)		; Is DMA active?
000D0E3A                            	else
000D0E3A                            		move.w	VDPCTRL,d0		; Is DMA active?
000D0E3A                            		btst	#1,d0
000D0E3A                            	endif
000D0E3A                            	bne.s	.Wait\@				; If so, wait
000D0E3A                            	endm
