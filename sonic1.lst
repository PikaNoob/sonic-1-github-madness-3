000D0E3A                            ; ---------------------------------------------------------------------------
000D0E3A                            ; VDP command set
000D0E3A                            ; ---------------------------------------------------------------------------
000D0E3A                            
000D0E3A                            VDPCMD macro ins, addr, type, rwd, end, end2
000D0E3A                            	local	cmd
000D0E3A                            cmd	= (\type\\rwd\)|(((\addr)&$3FFF)<<16)|((\addr)/$4000)
000D0E3A                            	if narg=5
000D0E3A                            		\ins	#\#cmd,\end
000D0E3A                            	elseif narg>=6
000D0E3A                            		\ins	#(\#cmd)\end,\end2
000D0E3A                            	else
000D0E3A                            		\ins	cmd
000D0E3A                            	endif
000D0E3A                            	endm
