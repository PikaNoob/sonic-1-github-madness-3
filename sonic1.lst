000D0E3A                            ; ---------------------------------------------------------------------------
000D0E3A                            ; VDP DMA from 68000 memory to VDP memory
000D0E3A                            ; ---------------------------------------------------------------------------
000D0E3A                            ; PARAMETERS:
000D0E3A                            ;	src  - Source address in 68000 memory
000D0E3A                            ;	dest - Destination address in VDP memory
000D0E3A                            ;	len  - Length of data in bytes
000D0E3A                            ;	type - Type of VDP memory
000D0E3A                            ;	port - Address register for the VDP port
000D0E3A                            ; ---------------------------------------------------------------------------
000D0E3A                            ; kys
000D0E3A =FFFFF644                  vdpIntBuffer = $FFFFF644
000D0E3A                            
000D0E3A                            VDPDMA  macro src, dest, len, type, port
000D0E3A                            	; DMA data
000D0E3A                            	lea	VDPCTRL,\port
000D0E3A                            	move.l	#$94009300|((((\len)/2)&$FF00)<<8)|(((\len)/2)&$FF),(\port)
000D0E3A                            	move.l	#$96009500|((((\src)/2)&$FF00)<<8)|(((\src)/2)&$FF),(\port)
000D0E3A                            	move.w	#$9700|(((\src)>>17)&$7F),(\port)
000D0E3A                            	VDPCMD	move.w,\dest,\type,DMA,>>16,(\port)
000D0E3A                            	VDPCMD	move.w,\dest,\type,DMA,&$FFFF,vdpIntBuffer.w
000D0E3A                            	move.w	vdpIntBuffer.w,(\port)
000D0E3A                            	endm
000D0E3A                            
000D0E3A                            ; -------------------------------------------------------------------------
000D0E3A                            ; VDP DMA fill VRAM with byte
000D0E3A                            ; -------------------------------------------------------------------------
000D0E3A                            ; PARAMETERS:
000D0E3A                            ;	addr - Address in VRAM
000D0E3A                            ;	len  - Length of fill in bytes
000D0E3A                            ;	byte - Byte to fill VRAM with
000D0E3A                            ;	inc  - VDP autoincrement value
000D0E3A                            ;	port - Control port address register
000D0E3A                            ; -------------------------------------------------------------------------
000D0E3A                            
000D0E3A                            VDPFILL macro addr, len, byte, inc, port
000D0E3A                            	; DMA fill
000D0E3A                            	lea	VDPCTRL,\port
000D0E3A                            	move.w	#$8F00+\inc,(\port)
000D0E3A                            	move.l	#$93009400|((((\len)-1)&$FF00)>>8)|((((\len)-1)&$FF)<<16),(\port)
000D0E3A                            	move.w	#$9780,(\port)
000D0E3A                            	VDPCMD	move.l,\addr,VRAM,DMA,(\port)
000D0E3A                            	move.w	#(\byte)<<8,VDPDATA
000D0E3A                            	DMAWAIT	\port
000D0E3A                            	endm
