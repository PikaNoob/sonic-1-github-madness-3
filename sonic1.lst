000CB5DE                            ;	len	- Length of data in bytes
000CB5DE                            ;	type	- Type of VDP memory
000CB5DE                            ;	a6.l	- VDP control port
000CB5DE                            ; ---------------------------------------------------------------------------------------------------------------------------------------------------------
000CB5DE                            ; RETURNS:
000CB5DE                            ;	Nothing
000CB5DE                            ; ---------------------------------------------------------------------------------------------------------------------------------------------------------
000CB5DE                            dma68k		macro	src, dest, len, type
000CB5DE                            		move.l	#$94009300|((((\len)/2)&$FF00)<<8)|(((\len)/2)&$FF),(a6)
000CB5DE                            		move.l	#$96009500|((((\src)/2)&$FF00)<<8)|(((\src)/2)&$FF),(a6)
000CB5DE                            		move.w	#$9700|(((\src)>>17)&$7F),(a6)
000CB5DE                            		vdpCmd	move.w, \dest, \type, DMA, >>16, (a6)
000CB5DE                            		vdpCmd	move.w, \dest, \type, DMA, &$FFFF, -(sp)
000CB5DE                            		move.w	(sp)+,(a6)
